{
  "transform": {
    "^.+\\.ts$": "ts-jest",
    "^.+\\.svelte$": ["svelte-jester", { "preprocess": true }]
  },
  "moduleNameMapper": {
    "comps/(.*)": "<rootDir>/src/components/$1",
    "types/(.*)": "<rootDir>/src/types/$1"
  },
  "moduleFileExtensions": ["js", "ts", "svelte"],
  "setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect"]
}
